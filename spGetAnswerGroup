ALTER PROCEDURE spGetAnswerGroup
    @timestampStart DATETIME,
    @timestampEnd DATETIME,
    @idForm INT
AS
    SELECT
        idAnswer,
        [timeStamp]
    FROM 
        AnswerGroup
    WHERE
        fkForm = @idForm AND [timeStamp] >= @timestampStart AND [timeStamp] <= @timestampEnd
    ORDER BY 
        [timeStamp]

-- exec spGetAnswerGroup '2023-06-08 02:48:42','2023-06-09 02:48:43',1
